/**
 * And gate:
 * if (a and b) out = 1, else out = 0 
 */
CHIP And {
    IN a, b;
    OUT out;
    
    PARTS:
    // First, implement the NAND gate
    Nand(a=a, b=b, out=nandOut);
    
    // Then, negate the NAND output to get the AND result
    Not(in=nandOut, out=out);
}