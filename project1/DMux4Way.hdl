CHIP DMux4Way {
    IN in, sel[2];
    OUT a, b, c, d;

    PARTS:
    // First-stage demux: Split based on sel[1] (MSB)
    DMux(in=in, sel=sel[1], a=ab, b=cd);
    
    // Second-stage demux: Split ab into a and b using sel[0]
    DMux(in=ab, sel=sel[0], a=a, b=b);
    
    // Second-stage demux: Split cd into c and d using sel[0]
    DMux(in=cd, sel=sel[0], a=c, b=d);
}