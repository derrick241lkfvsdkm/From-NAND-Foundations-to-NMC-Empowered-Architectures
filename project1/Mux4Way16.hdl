CHIP Mux4Way16 {
    IN a[16], b[16], c[16], d[16], sel[2];
    OUT out[16];

    PARTS:
    // First stage: Select between a/b and c/d using sel[0]
    Mux16(a=a, b=b, sel=sel[0], out=ab);
    Mux16(a=c, b=d, sel=sel[0], out=cd);

    // Second stage: Select between the first-stage results using sel[1]
    Mux16(a=ab, b=cd, sel=sel[1], out=out);
}